    "AND": "x\u00b7y  (\u0141ukasiewicz/product t-norm)",
    "NOT": "1-x  (standard complement)",
    "OR": "x+y-xy  (probabilistic sum / de Morgan dual of product)",
    "XOR": "|x-y|  (symmetric difference)"
  },
